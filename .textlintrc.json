{
  "filters": {},
  "rules": {
    "no-todo": true,
    "no-start-duplicated-conjunction": true,
    "rousseau": {
      "showLevels": ["suggestion", "warning", "error"]
    },
    "common-misspellings": true,
    "unexpanded-acronym": {
      "min_acronym_len": 3
    }
  },
  "plugins": {
    "@textlint/markdown": {
      "extensions": [".mdx"]
    }
  }
}
